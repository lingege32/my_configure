debug_time=0
function preexec1() {
  if [[ $debug_time == 1 ]]; then
      echo "enter $1"
      timer=$(($(date +%s%0N)/1000000))
  fi
}

function precmd1() {
  if [[ $debug_time == 1 ]]; then
      if [ $timer ]; then
        now=$(($(date +%s%0N)/1000000))
        elapsed=$(($now-$timer))

        echo "${elapsed}"
        unset timer
      fi
  fi
}

