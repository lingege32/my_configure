hi normal         ctermbg=none
hi ALEWarningSign ctermbg=none guibg=#1c1c1c ctermfg=228   guifg=#ffff87
hi ALEErrorSign   ctermbg=none guibg=#1c1c1c ctermfg=196   guifg=#ff0000
hi SignColumn     ctermbg=none guibg=#1c1c1c
hi Todo           ctermfg=226 guifg=#ffff00
hi LspWarningHighlight ctermbg=none guibg=#1c1c1c ctermfg=228   guifg=#ffff87
hi LspErrorHighLight   ctermbg=none guibg=#1c1c1c ctermfg=196   guifg=#ff0000
hi ClassMember    ctermfg=112 guifg=#87d7ff
hi LineNr         ctermbg=none
hi ObjectCtr      ctermfg=117 guifg=#87d7ff
hi Operator       ctermfg=206 guifg=#ff5fd7
hi constant       ctermfg=50  guifg=#00ffd7
hi cursorlinenr   ctermfg=226 guifg=#ffff00
hi CursorLine     term=underline ctermbg=234 guibg=#222222
hi specialchar    ctermfg=220 guifg=#E3CF57

